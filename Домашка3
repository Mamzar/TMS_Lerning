var array = [(2, "Slovo"), (12, "Neptun"), (11, "Voda"), (9, "Velo"), (6 , "Moto")]

func proba(mass: [(Int, String)]) {
var arInt: [Int] = []
var arString: [String] = []
 for (first,second) in mass {
   let kvadrat = first*first
   arInt.append(kvadrat)
   arString.append(second)
 }
var filteredNewArray = arInt.filter {element in element % 2 == 0}
var sortedarString = arString.sorted {element1, element2 in element1 < element2}
}   
proba(mass: array)
